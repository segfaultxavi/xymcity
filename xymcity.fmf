game xymcity
name "XYM City"
basegame id1
basegame xymcity

bind w +forward
bind s +back
bind a +moveleft
bind d +moveright
bind mouse2 +jump
bind mouse3 +forward
bind mwheelup impulse 10
bind mwheelup impulse 12

package id1/pak0.pak - "/id1/pak0.pak"
package xymcity/xymcity.pk3 - "xymcity/xymcity.pk3"
